{
  "name": "page-skeleton-webpack-plugin",
  "version": "0.10.1",
  "description": "web site skeleton generator",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src client/{index,sockjs,utils}.js",
    "beautify": "npm run lint -- --fix",
    "mocha": "mocha --full-trace --check-leaks",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "test": "npm run lint && npm run mocha",
    "dev:preview": "webpack --color --config preview/webpack.preview.config.js --watch",
    "dev:client": "webpack ./client/index.js client/index.bundle.js --color --config client/webpack.config.js --watch",
    "dev:sale": "cd examples/sale && npm run dev",
    "dev:script": "rollup --config src/script/rollup.config.js --watch",
    "build:index": "webpack ./client/index.js client/index.bundle.js --color --config client/webpack.config.js",
    "build:sockjs": "webpack ./client/sockjs.js client/sockjs.bundle.js --color --config client/webpack.sockjs.config.js",
    "build:preview": "webpack --color --config preview/webpack.preview.config.js",
    "build:script": "rollup --config src/script/rollup.config.js",
    "prepare": "npm run build:index && npm run build:sockjs && npm run build:preview && npm run build:script"
  },
  "keywords": [
    "puppeteer",
    "page skeleton",
    "webpack"
  ],
  "author": "jocs",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.3.0",
    "codemirror": "^5.36.0",
    "css-tree": "^1.0.0-alpha.27",
    "csso": "^3.3.1",
    "element-ui": "^2.3.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-extra": "^4.0.2",
    "hasha": "^3.0.0",
    "html-minifier": "^3.5.6",
    "html-webpack-plugin": "^2.30.1",
    "html2json": "^1.0.2",
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.0.1",
    "install": "^0.11.0",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.5",
    "memory-fs": "^0.4.1",
    "npm": "^5.8.0",
    "opn": "^5.1.0",
    "puppeteer": "^1.3.0",
    "qrcode": "^1.2.0",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.1.4",
    "tiza": "^2.1.0",
    "vue": "^2.5.2",
    "vuex": "^3.0.1",
    "webpack": "^3.8.1",
    "webpack-log": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-scm": "^1.0.9",
    "eslint-config-webpack": "^1.2.5",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-vue": "^2.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^4.0.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^2.1.2",
    "raw-loader": "^0.5.1",
    "rollup": "^0.57.1",
    "sinon": "^4.1.3",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2"
  },
  "eslintIgnore": [
    "templates"
  ]
}
